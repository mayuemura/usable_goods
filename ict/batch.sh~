#1
cat DP_EMCV_1 DP_EMCV_2 DP_EMCV_3 DP_EMCV_4 DP_EMCV_5 DP_EMCV_6 DP_EMCV_7 DP_EMCV_8 DP_EMCV_9 | ./feature.py > DP_EMCV_train.f

~okazaki/local/bin/crfsuite learn -a ap -p max_iterations=20 -m emt.model DP_EMCV_train.f

./feature.py < DP_EMCV_10 > DP_EMCV_test.f

~okazaki/local/bin/crfsuite tag -r -m emt.model < DP_EMCV_test.f > taggedDP

cat taggedDP | awk -F"\t" {'print $1'} > gold.txt

cat taggedDP | awk -F"\t" {'print $2'} > system.txt

python lenient_F.py gold.txt system.txt >> lenient_log.txt

#2
cat DP_EMCV_2 DP_EMCV_3 DP_EMCV_4 DP_EMCV_5 DP_EMCV_6 DP_EMCV_7 DP_EMCV_8 DP_EMCV_9 DP_EMCV_10 | ./feature.py > DP_EMCV_train.f

~okazaki/local/bin/crfsuite learn -a ap -p max_iterations=20 -m emt.model DP_EMCV_train.f

./feature.py < DP_EMCV_1 > DP_EMCV_test.f

~okazaki/local/bin/crfsuite tag -r -m emt.model < DP_EMCV_test.f > taggedDP

cat taggedDP | awk -F"\t" {'print $1'} > gold.txt

cat taggedDP | awk -F"\t" {'print $2'} > system.txt

python lenient_F.py gold.txt system.txt >> lenient_log.txt

#3
